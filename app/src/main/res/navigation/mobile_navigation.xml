        android:name="com.apiman.go4lunch.fragments.ListViewFragment"
        android:label="@string/title_list_view"
        tools:layout="@layout/fragment_list_view" />

    <fragment
        android:id="@+id/navigation_workmates"
        android:name="com.apiman.go4lunch.fragments.WorkmatesFragment"
        android:label="@string/title_workmates_available"
